<h1>Display Products</h1>
<a href="/" class="button">Home</a>
</br>
</br>

<div>
    <table>
        <thead>
            <tr>
                <th>productID</th>
                <th>supplierID</th>
                <th>Name</th>
                <th>Price</th>
            </tr>
        </thead>

        <tbody>
            {{#each products}}
            <tr>
                <td>{{productID}}</td>
                <td>{{supplierID}}</td>
                <td>{{productName}}</td>
                <td>{{productPrice}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<div>
    </br>
    <a href="/products/add_products" id="add_products">Add Products</a>
</div>

